{
  "name": "blog",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky install && gitmoji -i",
    "wasm": "wasm-pack build ./wasm --target web",
    "dev": "vite --open",
    "prebuild": "vue-tsc --noEmit && pnpm test:unit",
    "build": "vite-ssg build",
    "predeploy": "pnpm build",
    "deploy": "gh-pages -d dist",
    "preview": "vite preview --port 5002",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest watch",
    "extract:points": "tsnd --transpile-only -P tsconfig.common.json scripts/extract-points/index.ts",
    "extract:i18n": "tsnd --transpile-only -P tsconfig.common.json scripts/extract-i18n.ts && prettier --write locales",
    "upload:images": "tsnd --transpile-only -P tsconfig.common.json scripts/upload-images.ts"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.7",
    "@sentry/tracing": "^6.19.7",
    "@sentry/vue": "^6.19.7",
    "@supabase/supabase-js": "^1.30.0",
    "@tanstack/vue-query": "^4.20.5",
    "@vueuse/core": "^9.10.0",
    "@vueuse/head": "^1.0.22",
    "d3": "^7.8.0",
    "mapbox-gl": "^2.7.0",
    "nprogress": "^0.2.0",
    "vue": "^3.2.45",
    "vue-country-flag-next": "^2.3.2",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.11",
    "@intlify/unplugin-vue-i18n": "^0.8.1",
    "@types/d3": "^7.4.0",
    "@types/fs-extra": "^9.0.13",
    "@types/mapbox-gl": "^2.6.1",
    "@types/markdown-it-link-attributes": "^3.0.1",
    "@types/node": "^17.0.14",
    "@types/nprogress": "^0.2.0",
    "@types/object-path": "^0.11.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/test-utils": "^2.2.6",
    "axios": "^0.26.1",
    "chalk": "4.1.2",
    "critters": "^0.0.16",
    "dotenv": "^16.0.0",
    "form-data": "^4.0.0",
    "fs-extra": "^10.0.0",
    "gh-pages": "^3.2.3",
    "gitmoji-cli": "^4.7.0",
    "gpxparser": "^3.0.8",
    "happy-dom": "^2.39.1",
    "husky": "^7.0.4",
    "markdown-it-link-attributes": "^4.0.0",
    "markdown-it-prism": "^2.2.2",
    "object-path": "^0.11.8",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.9.4",
    "unplugin-auto-import": "^0.12.0",
    "unplugin-icons": "^0.14.15",
    "unplugin-vue-components": "^0.22.12",
    "unplugin-vue-macros": "^1.3.0",
    "vite": "^4.0.4",
    "vite-plugin-pages": "^0.28.0",
    "vite-plugin-vue-layouts": "^0.7.0",
    "vite-plugin-vue-markdown": "^0.22.2",
    "vite-plugin-wasm-pack": "^0.1.9",
    "vite-plugin-windicss": "^1.8.10",
    "vite-ssg": "^0.22.1",
    "vitest": "^0.26.2",
    "vue-i18n-extract": "^2.0.4",
    "vue-tsc": "^1.0.22",
    "windicss": "^3.5.6"
  }
}
