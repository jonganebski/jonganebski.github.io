{
  "name": "blog",
  "version": "0.0.0",
  "scripts": {
    "prepare": "husky install && gitmoji -i",
    "wasm": "wasm-pack build ./wasm --target web",
    "dev": "vite --open",
    "prebuild": "vue-tsc --noEmit && pnpm test:unit",
    "build": "vite-ssg build",
    "predeploy": "pnpm build",
    "deploy": "gh-pages -d dist",
    "preview": "vite preview --port 5002",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest watch",
    "extract:points": "tsnd --transpile-only -P tsconfig.common.json scripts/extract-points/index.ts",
    "extract:i18n": "tsnd --transpile-only -P tsconfig.common.json scripts/extract-i18n.ts && prettier --write locales",
    "upload:images": "tsnd --transpile-only -P tsconfig.common.json scripts/upload-images.ts"
  },
  "dependencies": {
    "@headlessui/vue": "^1.5.0",
    "@supabase/supabase-js": "^1.30.0",
    "@vueuse/core": "^7.5.5",
    "@vueuse/head": "^0.7.5",
    "d3": "^7.3.0",
    "gsap": "^3.9.1",
    "mapbox-gl": "^2.7.0",
    "vue": "^3.2.31",
    "vue-country-flag-next": "^2.3.2",
    "vue-i18n": "^9.2.0-beta.30",
    "vue-query": "^1.19.0",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.0.14",
    "@intlify/vite-plugin-vue-i18n": "^3.2.1",
    "@types/d3": "^7.1.0",
    "@types/fs-extra": "^9.0.13",
    "@types/mapbox-gl": "^2.6.1",
    "@types/markdown-it-link-attributes": "^3.0.1",
    "@types/node": "^17.0.14",
    "@types/object-path": "^0.11.1",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vue/test-utils": "^2.0.0-rc.20",
    "axios": "^0.26.1",
    "chalk": "4.1.2",
    "critters": "^0.0.16",
    "dotenv": "^16.0.0",
    "form-data": "^4.0.0",
    "fs-extra": "^10.0.0",
    "gh-pages": "^3.2.3",
    "gitmoji-cli": "^4.7.0",
    "gpxparser": "^3.0.8",
    "happy-dom": "^2.39.1",
    "husky": "^7.0.4",
    "markdown-it-link-attributes": "^4.0.0",
    "markdown-it-prism": "^2.2.2",
    "object-path": "^0.11.8",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4",
    "unplugin-auto-import": "^0.5.11",
    "unplugin-icons": "^0.13.0",
    "unplugin-vue-components": "^0.17.15",
    "vite": "^2.7.13",
    "vite-plugin-md": "^0.11.7",
    "vite-plugin-pages": "^0.20.1",
    "vite-plugin-vue-layouts": "^0.5.0",
    "vite-plugin-wasm-pack": "^0.1.9",
    "vite-plugin-windicss": "^1.6.3",
    "vite-ssg": "^0.17.9",
    "vitest": "^0.4.1",
    "vue-i18n-extract": "^2.0.4",
    "vue-tsc": "^0.29.8",
    "windicss": "^3.4.3"
  }
}
